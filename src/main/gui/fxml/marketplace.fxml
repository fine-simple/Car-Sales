<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxHeight="500.0" maxWidth="700.0" minHeight="500.0" minWidth="700.0" prefHeight="500.0" prefWidth="700.0" stylesheets="@../stylesheets/dark-theme.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.Marketplace">
   <children>
      <ImageView fitHeight="80.0" fitWidth="97.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../assets/Logo.png" />
         </image>
         <HBox.margin>
            <Insets left="20.0" top="20.0" />
         </HBox.margin>
      </ImageView>
      <VBox alignment="TOP_CENTER" maxHeight="500.0" maxWidth="700.0" minHeight="500.0" minWidth="588.0" prefHeight="500.0" prefWidth="588.0" spacing="20.0" stylesheets="@../stylesheets/dark-theme.css">
         <children>
            <HBox prefHeight="30.0" prefWidth="647.0">
               <children>
                  <TextField maxHeight="30.0" prefHeight="30.0" prefWidth="397.0" promptText="Search here ..." />
                  <Button maxHeight="30.0" maxWidth="150.0" minHeight="30.0" minWidth="150.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="150.0" text="Search" />
               </children>
            </HBox>
            <HBox alignment="CENTER" />
            <ScrollPane prefHeight="450.0" prefWidth="630.0">
               <content>
                  <VBox fx:id="list" prefHeight="750.0" prefWidth="496.0" />
               </content>
            </ScrollPane>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets bottom="35.0" left="35.0" right="35.0" top="35.0" />
         </padding>
      </VBox>
   </children>
</HBox>
