            <Insets left="80.0" right="80.0" />
         </VBox.margin></TextField>
      <TextField accessibleRole="PASSWORD_FIELD" maxHeight="30.0" maxWidth="300.0" minHeight="30.0" minWidth="250.0" prefHeight="30.0" prefWidth="275.0" promptText="Password">
         <VBox.margin>
            <Insets left="80.0" right="80.0" />
         </VBox.margin></TextField>
      <Button maxHeight="30.0" maxWidth="300.0" minHeight="30.0" minWidth="100.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="350.0" text="Login">
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin></Button>
      <Button maxHeight="30.0" maxWidth="400.0" minHeight="30.0" minWidth="100.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="350.0" text="Register">
         <VBox.margin>
            <Insets left="150.0" right="150.0" />
         </VBox.margin></Button>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets left="35.0" right="35.0" top="35.0" />
   </padding>
</VBox>
